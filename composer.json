{
    "name": "Zalazdi/larav",
    "description": "Laravel 5 IMAP client.",
    "keywords": [
        "laravel",
        "imap",
        "mail"
    ],
    "homepage": "https://github.com/Zalazdi/LaravelImap",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzysztof Graman",
            "email": "zalazdi@gmail.com",
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Zalazdi\\LaravelImap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zalazdi\\LaravelImap\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
